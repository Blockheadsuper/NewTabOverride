{
    "name": "New tab URL setter",
    "version": "1.0",
    "manifest_version": 3,
    "description": "Allows the user to choose their own new tab URL.",
    "background": {
        "service_worker": "background.js"
      },
    "options_ui": {
      "page": "options.html",
      "open_in_tab": false
    },
    "icons": {
      "16": "/icons/icon_16.png",
      "32": "/icons/icon_32.png",
      "64": "/icons/icon_64.png",
      "128": "/icons/icon_128.png",
      "256": "/icons/icon_256.png",
      "512": "/icons/icon_512.png"
    },
    "permissions": [
      "tabs",
      "storage"
    ]
  }